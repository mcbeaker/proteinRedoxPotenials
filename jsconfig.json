{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "paths": {"stream": ["./node_modules/readable-stream"]}
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}